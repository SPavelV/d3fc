{
  "name": "d3fc-annotation",
  "description": "A collection of D3 components for rendering plot area annotations to SVG, including lines, crosshairs, gridlines and more",
  "license": "MIT",
  "keywords": [
    "d3",
    "d3fc",
    "annotation",
    "crosshair"
  ],
  "homepage": "https://github.com/d3fc/d3fc-annotation",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3fc/d3fc-annotation"
  },
  "scripts": {
    "bundle": "rimraf build && rollup -c && uglifyjs build/d3fc-annotation.js --compress --mangle -o build/d3fc-annotation.min.js",
    "test": "npm run bundle && jasmine JASMINE_CONFIG_PATH=test/support/jasmine.json"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.9.0",
    "eslint": "^3.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "jasmine": "^2.4.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.4.0",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {
    "d3-scale": "^1.0.1",
    "d3fc-data-join": "^3.0.0",
    "d3fc-shape": "^4.0.0",
    "jsdom": "^9.4.1"
  }
}
